      <Button fx:id="prekiniBtn" mnemonicParsing="false" onMouseClicked="#prekiniBtn" text="Prekini" GridPane.columnIndex="3" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
